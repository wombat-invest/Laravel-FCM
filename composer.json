{
    "name": "wombat-invest/laravel-fcm",
    "description": "Laravel / Lumen package for Firebase Cloud Messaging ",
    "keywords": ["laravel", "lumen", "firebase", "notification", "push", "fcm", "firebase cloud messaging"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ash Phoenix",
            "email": "ashley.phoenix@wombatinvest.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "monolog/monolog": "^1.0|^2.0"
    },
    "require-dev": {
        "mockery/mockery" : "~1.0",
        "phpunit/phpunit" : "^9.0",
        "php-coveralls/php-coveralls": "^2.0",
        "laravel/laravel": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelFCM\\": "src/",
            "LaravelFCM\\Mocks\\": "tests/mocks"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFCM\\FCMServiceProvider"
            ],
            "aliases": {
                "FCM": "LaravelFCM\\Facades\\FCM",
                "FCMGroup": "LaravelFCM\\Facades\\FCMGroup"
            }
        }
    }
}
